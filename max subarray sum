// Online Java Compiler
// Use this editor to write, compile and run your Java code online
public class Main {
  public static void main(String[] args) {
    int a[]={-20,30,40,-10,50,-100,70};
    int cs=0,max=Integer.MIN_VALUE;

    int n=a.length;
    for(int i=0;i<n;i++)
    {
      cs=cs+a[i];
      max=Math.max(max,cs);
      if(cs<0)
      {
        cs=0;
      }
    }
      System.out.println(max);
  }
}
