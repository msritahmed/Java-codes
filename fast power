// Online Java Compiler
// Use this editor to write, compile and run your Java code online
public class Main {
  public static int p(int a,int n)
  {
    if(n==0)
    {
      return 1;

    }
    int pa=p(a,n/2);
    if(n%2==0)
    {
      return pa*pa;
    }
    return pa*pa*a;
  }
  public static void main(String[] args) {

      System.out.println(p(3,2));
  }
}
