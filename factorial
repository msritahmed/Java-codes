// Online Java Compiler
// Use this editor to write, compile and run your Java code online
public class Main {
  public static int p(int n)
  {
    if(n==0)
    {
      return 1;

    } 
    return n*p(n-1);
    }
  public static void main(String[] args) {
    
      System.out.println(p(3));
  }
}
