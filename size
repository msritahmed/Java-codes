import java.util.*;
public class Main {
  public static void main(String[] args) {
     int a[]={1,2,4,5,5,3,2,1,2,4};
     HashSet<Integer> m=new HashSet<>();
     for(int i=0;i<a.length;i++)
     {
      m.add(a[i]);
     }
      System.out.println(m.size());
  }
}
