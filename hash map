import java.util.*;

public class Main {
  public static void main(String[] args) {
    HashMap<Integer,Integer> m=new HashMap<>();
    m.put(2,3);
    m.put(1,2);
    m.put(2,4);
    m.remove(1);
    m.put(3,4);
    System.out.println(m.containsKey(3));//true
    System.out.println(m.get(3));//4
     System.out.println(m.size());//2
     System.out.println(m);
  }
}
