public class Main {
  public static int p(int a[],int i)
  {
    if(i==a.length)
    {
      return 0;

    }
    return a[i]+p(a,i+1);
  }
  public static void main(String[] args) {
    int a[]={1,2,3};

      System.out.println(p(a,0));
  }
}
